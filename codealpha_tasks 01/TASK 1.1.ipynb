   "source": [
    "\n",
    "from scapy.all import sniff, IP\n",
    "\n",
    "def packet_callback(packet):\n",
    "    if IP in packet:\n",
    "        print(f\"[+] {packet[IP].src} -> {packet[IP].dst}\")\n",
    "\n",
    "# Sniff 10 packets on the default interface\n",
    "sniff(filter=\"ip\", prn=packet_callback,count=10)"
   ]
